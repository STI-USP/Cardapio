rules:
  - LongLine
  - LongMethod
  - HighCyclomaticComplexity
  - DeepNestedBlock
  - TooManyParameters
  - TooManyMethods
  - HighNPathComplexity
  - LongClass
  
rule-configurations:
  - key: CYCLOMATIC_COMPLEXITY
    value: 10
  - key: LONG_LINE
    value: 120
  - key: LONG_METHOD
    value: 50
  - key: TOO_MANY_PARAMETERS
    value: 5
  - key: NPATH_COMPLEXITY
    value: 200
  - key: LONG_CLASS
    value: 500
    
disable-rules:
  - ShortVariableName

max-priority-1: 0
max-priority-2: 10
max-priority-3: 20

output: oclint-report.html
